{
  "name": "starter-kit",
  "version": "1.0.0",
  "description": "Starter Kit for NodeJs",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "./node_modules/.bin/tslint --fix -c  tslint.json 'src/**/**.ts'",
    "tsc": "tsc --project ./tsconfig.json",
    "prettier-all": "./node_modules/.bin/prettier --write 'src/**/*.+(ts|tsx|js|css|json)'",
    "precompile": "npm run prettier-all && npm run lint && npm run clean",
    "compile": "npm run tsc",
    "build": "npm run compile",
    "postbuild": "rimraf  dist/**/spec dist/**/*.spec.js && cp -r apidoc.json dist/apidoc.json && apidoc -i dist/controllers -o dist/public/apidoc",
    "watch": "tsc -w -p ./src -p ./tsconfig.json",
    "dev": "nodemon ./src/index.ts",
    "dev:debug": "export DEBUG_PORT=9229 && nodemon ./src/index.ts",
    "start": "node ./dist/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js -f src/typeorm/config/ormconfig.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "prettier": {
    "trailingComma": "none",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^15.14.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-clean-code": "^0.2.10",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  },
  "keywords": [
    "Shopify",
    "API",
    "Client"
  ],
  "author": "Yash Shah",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^6.13.3",
    "apidoc": "^0.29.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "bull": "^4.1.0",
    "camelcase-keys": "^7.0.1",
    "cluster": "^0.7.7",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "discord-webhook-node": "^1.1.8",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mailer": "^0.3.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "json2xls": "^0.1.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "mysql2": "^2.3.3",
    "path": "^0.12.7",
    "pg": "^7.12.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.4",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  }
}
